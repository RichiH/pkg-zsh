#!/bin/sh -e

case "$1" in
    remove|deconfigure)
	install-info --quiet --remove /usr/share/info/zsh.info.gz
	if test -x /usr/sbin/install-docs ; then install-docs -r zsh ; fi
    ;;
    upgrade)
	if test -x /usr/sbin/install-docs ; then install-docs -r zsh ; fi
    ;;

    failed-upgrade)
    ;;

    *)
	echo "prerm called with unknown argument \`$1'" >&2
	exit 0
    ;;
esac
